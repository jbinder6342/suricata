#*****************************
# cyos rules for file extraction... 
# 
# Example files.rules for other examples 

#---------------------
# cyos: Store all "application types" 
#
#alert http any any -> any any (msg:"FILE store all"; flow:established; filestore; sid:1; rev:10000;)

#---------------------
# specific types 
#
alert http any any -> any any (msg:"FILE EXE file claimed"; fileext:"exe"; filestore; sid:10002; rev:1;)
alert http any any -> any any (msg:"FILE PDF file claimed"; fileext:"pdf"; filestore; sid:10003; rev:1;)
alert http any any -> any any (msg:"FILE SWF file claimed"; fileext:"swf"; filestore; sid:10004; rev:1;)

alert http any any -> any any (msg:"FILE PE32 detected"; filemagic:"PE32"; filestore; sid:11005; rev:1;)
alert http any any -> any any (msg:"FILE PDF detected"; filemagic:"PDF document"; filestore; sid:11006; rev:1;)
alert http any any -> any any (msg:"FILE Flash detected" filemagic:"Macromedia*Flash*"; filestore; sid:11007; rev:1;)

alert http any any -> any any (msg:"FILE magicType -- applicaiton";  filemagic:"application/"; filestore; sid:12000; rev:1;)

# example of excluding a domain. 
#alert http any any -> any any (msg:"FILE magicType -- applicaiton"; content:!"safebrowsing-cache.google.com"; nocase; http_header;  filemagic:"application/"; flow:established; filestore; sid:12001; rev:1;)  

alert http any any -> any any (msg:"FILE magicType -- application/javascript"; filemagic:"application/javascript"; filestore; sid:12002; rev:1;)
alert http any any -> any any (msg:"FILE magicType -- application/x-javascript"; filemagic:"application/x-javascript"; filestore; sid:12003; rev:1;)
alert http any any -> any any (msg:"FILE magicType -- text/javascript"; filemagic:"text/html"; filestore; sid:12004; rev:1;)
alert http any any -> any any (msg:"FILE magicType -- text/html"; filemagic:"text/html"; filestore; sid:12005; rev:1;)

alert http any any -> any any (msg:"FILE magicType -- application/dosexec detected"; filemagic:"application/dosexec"; filestore; sid:13001; rev:1;)
alert http any any -> any any (msg:"FILE magicType -- application/x-dosexec detected"; filemagic:"application/x-dosexec"; filestore; sid:13002; rev:1;)
